<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity JobAddress.
    -->
    <changeSet id="20171120085936-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="address_id"
                                 baseTableName="job_address"
                                 constraintName="fk_job_address_address_id"
                                 referencedColumnNames="id"
                                 referencedTableName="address"/>

        <addForeignKeyConstraint baseColumnNames="school_id"
                                 baseTableName="job_address"
                                 constraintName="fk_job_address_school_id"
                                 referencedColumnNames="id"
                                 referencedTableName="mjob"/>

    </changeSet>
</databaseChangeLog>
